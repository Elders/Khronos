{
  "manifest_version": 2,
  "name": "Test Extension",
  "version": "1.0.0",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "https://marketvision.atlassian.net/*"
  ],
  "background": {
    "page": "index.html#/dashboard",
    "persistent": false
  },
  "options_page": "index.html#/settings",
  "browser_action": {
    "default_title": "Elders Khronos Time Logging",
    "default_popup": "index.html#dashboard"
  },
  "icons": {},
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}